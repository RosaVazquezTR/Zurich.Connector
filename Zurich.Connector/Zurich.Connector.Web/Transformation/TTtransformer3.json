{
  "Documents": {
    "#loop($.Documents)": {
      "documentId": "#currentvalueatpath($.id)",
      "name": "#currentvalueatpath($.name)",
      "createdOn": "#currentvalueatpath($.createdOn)",
      "documentTypeId": "#currentvalueatpath($.documentTypeId)",
      "pageCount": "#currentvalueatpath($.pageCount)",
      "lastModifiedOn": "#currentvalueatpath($.lastModifiedOn)",
      "processingStatus": "#currentvalueatpath($.processingStatus)",
      "highlightedText": "#currentvalueatpath($.highlightedText)",
      "thoughts": {
        "#loopwithincontext($.thoughts)": {
          "id": "#currentvalueatpath($.id)",
          "clauseTypeId": "#currentvalueatpath($.thoughtTypeId)",
          "fields": {
            "#loopwithincontext($.fields)": {
              "name": "",
              "createdOn": "",
              "documentTypeId": "",
              "pageCount": "",
              "lastModifiedOn": "",
              "processingStatus": "",
              "highlightedText": "#currentvalueatpath($.highlightedText)",
              "thoughtId": "",
              "clauseTypeId": "",
              "fieldId": "#currentvalueatpath($.id)",
              "clauseTermId": "#currentvalueatpath($.thoughtFieldTypeId)",
              "score": "#currentvalueatpath($.score)",
              "confidence": "#currentvalueatpath($.confidence)",
              "stringValue": "#currentvalueatpath($.stringValue)"
            }
          }
        }
      }
    }
  }
}